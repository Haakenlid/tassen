{
  "name": "universitas.no",
  "author": "HÃ¥ken Lid",
  "license": "ISC",
  "version": "1.2.0",
  "description": "frontend universitas",
  "scripts": {
    "buildssr": "webpack-cli --config webpack.server.config.js ",
    "serve": "TARGET=server node ./build/server.bundle.js",
    "test": "jest --watchAll --verbose",
    "testonce": "jest",
    "build": "webpack-cli --config webpack.prod.config.js ",
    "tern": "tern --persistent --host 0.0.0.0 --port 33000",
    "rebuild": "npm run build",
    "stats":
      "webpack-cli --config webpack.prod.config.js --profile --json > webpack-stats.json",
    "dev": "webpack-dev-server --config webpack.dev.config.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/universitas/universitas.no/"
  },
  "jest": {
    "verbose": true,
    "setupFiles": ["<rootDir>/jest.setup.js"],
    "moduleDirectories": ["node_modules", "src/react"],
    "moduleNameMapper": {
      "\\.(css|scss|sass|less)$": "<rootDir>/__mocks__/css.jest.mock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/file.jest.mock.js"
    },
    "roots": ["<rootDir>/src/react/", "<rootDir>/src/storybooks/"]
  },
  "dependencies": {
    "ajv": "^6.2.1",
    "babel-core": "^6",
    "babel-jest": "^22",
    "babel-loader": "^7.1.4",
    "babel-plugin-import-asserts": "^3",
    "babel-plugin-lodash": "^3.3.2",
    "babel-polyfill": "^6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6",
    "babel-preset-stage-0": "^6",
    "blueimp-load-image": "^2.18.0",
    "classnames": "^2",
    "css-loader": "^0.28.10",
    "cuid": "^2",
    "date-fns": "^1.29.0",
    "enzyme": "^3.3.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1",
    "foundation-sites": "^6",
    "fuzzyset": "0.0.4",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2",
    "js-cookie": "^2.2.0",
    "json-stringify-pretty-compact": "^1.2.0",
    "localforage": "^1.6",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "node-sass": "^4.9.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.1.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "prop-types": "^15.6.1",
    "ramda": "^0.25",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-error-boundary": "^1.2.3",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4",
    "react-icons": "^2.2.7",
    "react-redux": "^5",
    "redux": "^4",
    "redux-first-router": "0.0.9-rudy",
    "redux-first-router-link": "0.0.3-rudy",
    "redux-little-router": "^14",
    "redux-logger": "^3",
    "redux-persist": "^5",
    "redux-saga": "^0.16",
    "redux-thunk": "^2",
    "reselect": "^3",
    "sass-loader": "^7.0.1",
    "slick-carousel": "^1.8.1",
    "slugify": "^1.2.9",
    "spark-md5": "^3.0.0",
    "style-loader": "^0.21",
    "url-loader": "^1",
    "webpack": "^4.6.0",
    "webpack-bundle-tracker": "^0.3",
    "webpack-cli": "^2.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-server-middleware": "^0.5.0",
    "@storybook/addon-a11y": "^4.0.0-alpha.9",
    "@storybook/addon-actions": "^4.0.0-alpha.9",
    "@storybook/addon-knobs": "^4.0.0-alpha.9",
    "@storybook/addon-options": "^4.0.0-alpha.9",
    "@storybook/addons": "^4.0.0-alpha.9",
    "@storybook/react": "^4.0.0-alpha.9",
    "jest": "^22",
    "react-test-renderer": "^16.2.0",
    "storybook-addon-jsx": "^5.3.0",
    "tern": "^0.21"
  }
}
